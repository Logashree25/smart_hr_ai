{
  "welcomeFile": "/webapp/index.html",
  "authenticationMethod": "none",
  "routes": [
    {
      "source": "^/webapp/(.*)$",
      "target": "$1",
      "localDir": "webapp",
      "cacheControl": "no-cache, no-store, must-revalidate"
    },
    {
      "source": "^/hr/(.*)$",
      "target": "/hr/$1",
      "destination": "backend"
    },
    {
      "source": "^/genai/(.*)$",
      "target": "/genai/$1", 
      "destination": "backend"
    }
  ]
}
